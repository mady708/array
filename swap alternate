#include<iostream>
using namespace std;

void swapalternate(int arr[],int n)
{
   for(int i=0;i<n-1;i++)
   {
    swap(arr[i],arr[i+1]);
    i++;
   }
}
void printarray(int arr[],int n)
{
    for(int k=0;k<=n;k++)
    {
    cout<<arr[k];
    }
}
int main ()
{
    int arr[5]={1,2,3,4,5};
    swapalternate(arr,5);
    printarray(arr,5);
    return 0;
}    
